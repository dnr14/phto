CREATE TABLE CATEGROUP(
    categrpNo number(10) not null primary key, 
    categrpName varchar(50) not null, 
    categrpCnt number(7) default 0 not null,
    categrpSeqno number(7) default 0 not null, 
    categrpVisible char(1) default 'Y' not null, 
    categrpRdate date default sysdate not null 
);

CREATE TABLE stock(
    stockNo number(10) not null, -- 재고 고유번호
    stockCnt number(10) not null, -- 재고 수량
    stockName varchar(50) not null, -- 재고 이름
    stockPrice number(10) not null, -- 재고 가격
    categrpNo number(10) not null, -- 카테고리 번호
    manager varchar(20) not null, -- 재고 등록한 직원
    stockRdate date default sysdate not null, -- 재고 등록날짜
    primary key(stockNo),
    foreign key (categrpNo) references CATEGROUP(categrpNo)
);

